<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.bsproject.mapper.UserMapper">

    <select id="selectByUsernameUser" resultType="org.example.bsproject.dto.User">
        SELECT *
        FROM user
        WHERE username = #{username}
        LIMIT 1
    </select>

</mapper>
